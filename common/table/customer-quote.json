{
  "settings": {
    "tables":{
      "sort":{
        "added": "DESC"
      },
      "beforeLoad":{
        "currentBrandId": "$user.brandId",
        "cityId": "$user.cityId"
      },
      "edit":{
        "disableWhen": {
          "key": "$data.editButton",
          "value": "disable"
        },
        "display": true
      },
      "action":[
        {
          "title":"Quote Reply",
          "onClick":{
            "type":"dialog",
            "state":"dashboard.quoteReply",
            "options":{
              "customerQuoteId": "$data.id",
              "dealerId": "$user.id"
            },
            "icon": ["fa", "fa-mail-reply"],
            "dataTargetId": "customerQuoteReply"
          },
          "disableWhen": {
            "key": "$data.replyButton",
            "value": "disable"
          }
        }
      ]
    }
  },
  "widgets": [
    {
      "label": "Customer Quotes",
      "icon": "si-bar-chart",
      "fetchLocally": true,
      "propObj": {
        "type": "$allTime",
        "where": {},
        "dateProp": "added"
      }
    }
  ],

  "tables": {
    "added": {
      "date": true,
      "search": "date",
      "display":true,
      "label": "Date"
    },
    "quoteType": {
      "label": "Query",
      "position": 0,
      "style":{
        "width": "50px"
      },
      "tag": {
        "q": "label-primary",
        "t": "label-warning"
      },
      "search": "select",
      "options": [
        "q",
        "t"
      ]
    },
    "customer_firstName": {
      "position": 1,
      "label": "Customer",
      "style":{
        "width": "100px"
      },
      "search": "text"
    },
    "customer_lastName": {
      "display": false
    },
    "customer_email": {
      "display": false
    },
    "customer_username": {
      "display": false
    },
    "customer_phoneNumber": {
      "label": "Mobile",
      "position": 12,
      "style":{
        "width": "130px"
      },
      "search": "text"
    },
    "customer_profilePic": {
      "display": false
    },
    "customer_added": {
      "display": false
    },
    "customer_notificationSettings":{
      "display": false
    },
    "customer_gpsTrackerNotification":{
