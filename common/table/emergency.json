{
  "widgets": [
    {
      "label": "Emergencies",
      "icon": "si-bar-chart",
      "fetchLocally": true,
      "propObj": {
        "type": "$allTime",
        "where": {},
        "dateProp": "added"
      }
    }
  ],

  "tables": {
    "added": {
      "date": true,
      "search": "date"
    },
    "name": {
      "search": "text"
    },
    "contactNumber": {
      "search": "text",
      "label": "Contact"
    },
    "latlong": {
      "display": false
    },
    "city_added": {
      "display": false
    },
    "city_status": {
      "display": false
    },
    "city_name": {
      "label": "City",
      "search": "related",
      "type": "text",
      "display": true,
      "relatedModel": "City",
      "foreignKey": "cityId",
      "propertyName": "name"
    },
    "area_added": {
     "display": false
    },
    "area_pincode": {
      "display": false
    },
    "area_name": {
      "display": false,
      "label" : "Area",
      "search": "text"
    },
    "emergencyCategory_added": {
      "display": false
    },
    "emergencyCategory_name": {
      "label": "Category",
      "search": "related",
      "type": "text",
      "display": true,
      "relatedModel": "EmergencyCategory",
      "foreignKey": "emergencyCategoryId",
      "propertyName": "name"
    }
  }
}