{
  "widgets": [
    {
      "label": "Trim",
      "icon": "si-bar-chart",
      "fetchLocally": true,
      "propObj": {
        "type": "$allTime",
        "where": {},
        "dateProp": "added"
      }
    }
  ],

  "tables": {
    "name": {
      "search": "text"
    },
    "added": {
      "search": "date",
      "date": true
    },
    "status": {
      "tag": {
        "inactive": "label-warning",
        "active": "label-success"
      },
      "search": "select",
      "options": [
        "active",
        "inactive"
      ]
    },
    "brand_name": {
      "label": "Brand",
      "search": "related",
      "type": "text",
      "display": true,
      "relatedModel": "Brand",
      "foreignKey": "brandId",
      "propertyName": "name"
    },
    "brand_added": {
      "display": false
    },
    "brand_image": {
      "display": false
    },
    "brand_status": {
      "display": false
    },
    "carModel_name": {
      "label": "Model",
      "search": "related",
      "type": "text",
      "display": true,
      "relatedModel": "CarModel",
      "foreignKey": "carModelId",
      "propertyName": "name"
    },
    "carModel_added": {
      "display": false
    },
    "carModel_image": {
      "display": false
    },
    "carModel_quoteImage": {
      "display": false
    },
    "carModel_status": {
      "display": false
    },
    "fuel_added": {
      "display": false
    },
    "fuel_name": {
      "label": "Fuel",
      "search": "related",
      "type": "text",
      "display": true,
      "relatedModel": "Fuel",
      "foreignKey": "fuelId",
      "propertyName": "name"
    },
    "fuel_status": {
      "display": false
    },
    "gearBox_name": {
      "label": "GearBox",
      "search": "related",
      "type": "text",
      "display": true,
      "relatedModel": "GearBox",
      "foreignKey": "gearBoxId",
      "propertyName": "name"
    },
    "gearBox_status": {
      "display": false
    },
    "gearBox_added": {
      "display": false
    }
  }
}