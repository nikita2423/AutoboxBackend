{
  "settings": {
    "tables": {
      "sort":{
        "added": "DESC"
      },
      "beforeLoad": {
        "dealerId":"$user.id"
      },
      "action":[
        {
          "title": "Reply",
          "onClick": {
            "type": "dialog",
            "state": "customerCallMessage.replyToCustomer",
            "options":{
              "customerMessageId": "$data.id",
              "dealerId": "$user.id"
            },
            "icon": ["fa", "fa-mail-reply"],
            "dataTargetId": "customerMessageReply"
          }
        }
      ]
    }
  },
  "widgets": [
    {
      "label": "Customer Messages",
      "icon": "si-bar-chart",
      "fetchLocally": true,
      "propObj": {
        "type": "$allTime",
        "where": {},
        "dateProp": "added"
      }
    }
  ],
  "tables": {
     "customer_firstName": {
       "position":0,
       "search": "text",
       "label": "First Name"
     },
    "customer_lastName": {
      "display": true,
      "position":1,
      "search": "text",
      "label": "LastName"
    },
    "customer_added": {
      "display": false
    },
    "customer_status": {
      "display": false
    },
    "customer_email":{
      "display":false
    },
    "customer_phoneNumber": {
      "label": "Mobile",
      "search": "text"
    },
    "customer_username": {
      "display": false
    },
    "time":{
      "position":4,
      "display": true
    },
    "type": {
      "position":2,
      "tag": {
        "call": "label-primary",
        "message": "label-warning"
      },
      "search": "select",
      "options": [
        "call",
        "message"
      ]
    },
    "added": {
      "date": true,
      "search": "date",
      "label": "Date",
      "position":3,
      "format":"dd/mm/yyyy"
    },
    "status": {
      "tag": {
        "pending": "label-danger",
        "contacted": "label-success"
      },
      "position": 5,
      "search": "select",
      "options": [
        "pending",
        "contacted"
      ]
    },
    "message": {
      "search": "text",
      "position": 7
    },
    "subject": {
      "display": false
    },
    "userType": {
      "display": false
    },
    "dealer_firstName": {
      "display": false
    },
    "dealer_lastName": {
      "display": false
    },
    "dealer_status": {
      "display": false
    },
    "dealer_added": {
      "display": false
    },
    "dealer_email": {
      "display": false
    },
    "dealer_contact": {
      "display": false
    },
    "dealer_noOfVehicleSold":{
      "display": false
    },
    "customerQuote_ownershipType": {
      "display": false
    },
    "customerQuote_isFinance": {
      "display": false
    },
    "customerQuote_isInsurance": {
      "display": false
    },
    "customerQuote_isOldVehicleTrade": {
      "display": false
    },
    "customerQuote_quoteType": {
      "display": false
    },
    "customerQuote_soldViaAutobox": {
      "display":false
    },
    "customerQuote_gpsTracker": {
      "display": false
    },
    "customerQuote_dashCamera": {
      "display": false
    },
    "customerQuote_testDrive": {
      "display": false
    },
    "customerQuote_quoteNumber": {
      "display": false
    },
    "customerQuote_added": {
      "display": false
    },
    "customer_profilePic": {
      "display": false
    },
    "customer_sosStatus": {
      "display": false
    },
    "customer_registerStatus": {
      "display": false
    },
    "customer_countryName": {
      "display": false
    },
    "firstName": {
      "search": "text",
      "label": "First Name",
      "position":0
    },
    "lastName": {
      "search": "text",
      "label": "Last Name",
      "position":1
    },
    "mobileNumber": {
      "search": "text",
      "label": "Mobile Number",
      "position": 6
    }
  }
}