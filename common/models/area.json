{
  "name": "Area",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter name",
          "id": "areaName"
        }
      }
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "city": {
      "type": "belongsTo",
      "model": "City",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add City",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "create": false,
        "id": "AddCity",
        "hide": false,
        "label": "Add City"
      }
    },
    "country": {
      "type": "belongsTo",
      "model": "Country",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Country",
        "searchProp": "name",
        "priority": 1,
        "search": true,
        "create": false,
        "id": "AddCountry",
        "hide": false,
        "label": "Add Country"
      }
    }
  },
  "acls": [],
  "methods": {}
}
