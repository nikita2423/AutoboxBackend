{
  "name": "Breakdown",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "contactNumber": {
      "type": "string",
      "required": true
    },
    "latLong": {
      "type": "geopoint",
      "required": true
    },
    "breakdownNumber": {
      "type": "string",
      "required": true
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "area": {
      "type": "belongsTo",
      "model": "Area",
      "foreignKey": ""
    },
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": ""
    },
    "breakdownCategory": {
      "type": "belongsTo",
      "model": "BreakdownCategory",
      "foreignKey": ""
    },
    "city": {
      "type": "belongsTo",
      "model": "City",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
