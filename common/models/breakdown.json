{
  "name": "Breakdown",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter name",
          "id": "breakdownName"
        }
      }
    },
    "contactNumber": {
      "type": "string",
      "required": false,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter Contact Number",
          "id": "contactNumber"
        }
      }
    },
    "latlong": {
      "type": "geopoint",
      "required": false,
      "template": {
        "type": "objectValue",
        "templateOptions": {
          "fields": [
            {
              "type": "input",
              "key": "lat",
              "templateOptions": {
                "type": "text",
                "label": "Enter latitude",
                "id": "latitude"
              }
            },
            {
              "type": "input",
              "key": "lng",
              "templateOptions": {
                "type": "text",
                "label": "Enter longitude",
                "id": "longitude"
              }
            }
          ]
        }
      }
    },
    "breakdownNumber": {
      "type": "string",
      "required": false
    },
    "added": {
      "type": "date",
      "required": false,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "area": {
      "type": "belongsTo",
      "model": "Area",
      "foreignKey": ""
    },
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": ""
    },
    "breakdownCategory": {
      "type": "belongsTo",
      "model": "BreakdownCategory",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Breakdown Category",
        "searchProp": "name",
        "priority": 2,
        "type": "smartSelect",
        "search": true,
        "create": false,
        "id": "AddBreakdownCategory",
        "hide": false,
        "label": "Add Breakdown Category"
      }
    },
    "city": {
      "type": "belongsTo",
      "model": "City",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add City",
        "searchProp": "name",
        "priority": 2,
        "type": "smartSelect",
        "search": true,
        "create": false,
        "id": "AddCity",
        "hide": false,
        "label": "Add City"
      }
    }
  },
  "acls": [],
  "methods": {}
}
