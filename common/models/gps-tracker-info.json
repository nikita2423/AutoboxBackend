{
  "name": "GpsTrackerInfo",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "deviceIMEI": {
      "type": "string",
      "required": true
    },
    "gpsPassword": {
      "type": "string"
    },
    "registrationNumber": {
      "type": "string",
      "required": true
    },
    "serialNumber": {
      "type": "string",
      "required": false
    },
    "modelName": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "default": "active"
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "gpsTrackerNotification": {
      "type": "object",
      "required": false,
      "template": {
        "type": "objectValue",
        "templateOptions": {
          "fields": [
            {
              "key": "hardBraking",
              "type": "input",
              "default": "on"
            },
            {
              "key": "hardAcceleration",
              "type": "input",
              "default": "on"
            },
            {
              "key": "gpsDisconnect",
              "type": "input",
              "default": "on"
            },
            {
              "key": "vehicleTowing",
              "type": "input",
              "default": "on"
            },
            {
              "key": "overSpeeding",
              "type": "input",
              "default": "on"
            },
            {
              "key": "accidentDetection",
              "type": "input",
              "default": "on"
            }
          ]
        }
      }
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": ""
    },
    "vehicleDetail": {
      "type": "belongsTo",
      "model": "VehicleDetail",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
