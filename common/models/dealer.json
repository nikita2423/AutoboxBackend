{
  "name": "Dealer",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [
    "password"
  ],
  "properties": {
    "firstName": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "inline": true,
          "colSize": "col-xs-6",
          "label": "Enter First Name",
          "id": "firstName"
        }
      }
    },
    "lastName": {
      "type": "string",
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "inline": true,
          "colSize": "col-xs-6",
          "label": "Enter Last Name",
          "id": "lastName"
        }
      }
    },
    "password": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "password",
          "label": "Enter Password",
          "priority": 7,
          "inline": true,
          "colSize": "col-xs-6",
          "id": "password"
        }
      }
    },
    "email": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "email",
          "label": "Enter Dealer email",
          "id": "dealerEmail",
          "priority": 8,
          "inline": true,
          "colSize": "col-xs-6"
        }
      }
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "active",
      "template": {
        "type": "selectString",
        "templateOptions": {
          "label": "Status of Dealer",
          "id": "dealerStatus",
          "priority": 6,
          "inline": true,
          "colSize": "col-xs-6",
          "options": [
            "active",
            "inactive"
          ]
        }
      }
    },
    "contact": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "label": "Enter Contact Number",
          "id": "dealerContact",
          "priority": 8,
          "inline": true,
          "colSize": "col-xs-6"
        }
      }
    },
    "noOfVehicleSold": {
      "type": "number",
      "required": true,
      "default": 0,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "number",
          "inline": true,
          "colSize": "col-xs-6",
          "label": "Enter Number Of Vehicles Sold",
          "id": "vehicleSold"
        }
      }
    },

    "dealerNumber": {
      "type": "string",
      "required": true
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "area": {
      "type": "belongsTo",
      "model": "Area",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Area",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "type":"smartSelect",
        "create": false,
        "id": "AddArea",
        "hide": false,
        "inline": true,
        "colSize": "col-xs-6",
        "label": "Add Area"
      }
    },
    "city": {
      "type": "belongsTo",
      "model": "City",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add City",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "type":"smartSelect",
        "create": false,
        "id": "AddCity",
        "inline": true,
        "colSize": "col-xs-6",
        "hide": false,
        "label": "Add City"
      }
    },
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Brand",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "type":"smartSelect",
        "create": false,
        "inline": true,
        "colSize": "col-xs-6",
        "id": "AddBrand",
        "hide": false,
        "label": "Add Brand"
      }
    },
    "showrooms": {
      "type": "hasOne",
      "model": "Showroom",
      "foreignKey": ""
    },
    "workshops": {
      "type": "hasOne",
      "model": "Workshop",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "staff",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "staff",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "staff",
      "permission": "ALLOW",
      "property": "findById"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "staff",
      "permission": "ALLOW",
      "property": "findOne"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "isAdmin"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "getAbsoluteSchema"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "count"
    },
    {
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW",
      "property": "save"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "fetchDealerDetail"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "fetchDealersForBrand"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW",
      "property": "login"
    }
  ],
  "methods": {}
}
