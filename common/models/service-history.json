{
  "name": "ServiceHistory",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "dateOfBooking": {
      "type": "date",
      "required": true
    },
    "mileageCompleted": {
      "type": "number",
      "required": true
    },
    "charges": {
      "type": "number",
      "required": true
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": ""
    },
    "serviceBooking": {
      "type": "belongsTo",
      "model": "ServiceBooking",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
