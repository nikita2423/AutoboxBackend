{
  "name": "TrackBusVehicle",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "gpsCode": {
      "type": "string",
      "required": true
    },
    "homeLocation": {
      "type": "geopoint",
      "required": true,
      "template": {
        "type": "objectValue",
        "templateOptions": {
          "fields": [
            {
              "type": "input",
              "key": "lat",
              "templateOptions": {
                "type": "text",
                "label": "Enter latitude",
                "id": "latitude"
              }
            },
            {
              "type": "input",
              "key": "lng",
              "templateOptions": {
                "type": "text",
                "label": "Enter longitude",
                "id": "longitude"
              }
            }
          ]
        }
      }
    },
    "homeAddress": {
      "type": "string"
    },
    "gpsBusNotification": {
      "type": "object",
      "required": false,
      "template": {
        "type": "objectValue",
        "templateOptions": {
          "fields": [
            {
              "key": "busVicinity",
              "type": "input",
              "default": "on"
            }
          ]
        }
      }
    },
    "status": {
      "type": "string",
      "default": "active"
    },
    "vicinity": {
      "type": "number"
    },
    "deviceIMEI": {
      "type": "string"
    },
    "added": {
      "type": "date",
      "required": false,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": ""
    },
    "busModel": {
      "type": "belongsTo",
      "model": "BusModel",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
