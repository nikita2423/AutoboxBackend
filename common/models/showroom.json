{
  "name": "Showroom",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Showroom Name",
          "id": "showroomName",
          "box": "name",
          "inline": "true",
          "colSize": "col-md-4"
        }
      }
    },
    "salesConsultantName": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Sales Consultant Name",
          "id": "salesConsultantName",
          "box": "name",
          "inline": "true",
          "colSize": "col-md-4"
        }
      }
    },
    "contactNumber": {
      "type": "string",
      "required": false,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Contact Number",
          "id": "conatct",
          "box": "name",
          "inline": "true",
          "colSize": "col-md-4"
        }
      }
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "active",
      "template": {
        "type": "selectString",
        "templateOptions": {
          "label": "Status of Showroom",
          "id": "showroomStatus",
          "priority": 6,
          "options": [
            "active",
            "inactive"
          ]
        }
      }
    },
    "email": {
      "type": "string",
      "required": false,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "email",
          "priority": 3,
          "label": "Email",
          "id": "email"
        }
      }
    },
    "generalManagerEmail": {
      "type": "string",
      "required": false,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "email",
          "priority": 3,
          "label": "GM Email",
          "id": "gmemail"
        }
      }
    },
    "latlong": {
      "type": "geopoint",
      "required": true,
      "template": {
        "type": "objectValue",
        "templateOptions": {
          "fields": [
            {
              "type": "input",
              "key": "lat",
              "templateOptions": {
                "type": "text",
                "label": "Enter latitude",
                "id": "latitude"
              }
            },
            {
              "type": "input",
              "key": "lng",
              "templateOptions": {
                "type": "text",
                "label": "Enter longitude",
                "id": "longitude"
              }
            }
          ]
        }
      }
    },
    "address": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter address",
          "id": "Address"
        }
      }
    },
    "landline": {
      "type": "string",
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Landline",
          "id": "landline"
        }
      }
    },
    "image": {
      "type": "object",
      "required": true,
      "template": {
        "type": "singleFileUpload",
        "templateOptions": {
          "label": "Add Showroom Image",
          "containerName": "AMAZON_CONTAINER_NAME",
          "containerModel": "Container",
          "bind": true,
          "fileDataSource": "Image",
          "onImageUpdate": {
            "deletePrevious": true
          }
        }
      }
    },
    "managerImage": {
      "type": "object",
      "template": {
        "type": "singleFileUpload",
        "templateOptions": {
          "label": "Add Manager Image",
          "containerName": "AMAZON_CONTAINER_NAME",
          "containerModel": "Container",
          "bind": true,
          "fileDataSource": "Image",
          "onImageUpdate": {
            "deletePrevious": true
          }
        }
      }
    },
    "timings": {
      "type": "object",
      "required": true,
      "template": {
        "type": "objectValue",
        "templateOptions": {
          "fields": [
            {
              "type": "input",
              "key": "opening",
              "templateOptions": {
                "type": "time",
                "label": "Opening Timing",
                "id": "opening"
              }
            },
            {
              "type": "input",
              "key": "closing",
              "templateOptions": {
                "type": "time",
                "label": "Closing Timing",
                "id": "closing"
              }
            }
          ]
        }
      }
    },
    "showroomNumber": {
      "type": "string",
      "required": false
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "city": {
      "type": "belongsTo",
      "model": "City",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add City",
        "searchProp": "name",
        "type": "smartSelect",
        "readOnly": true,
        "inline": true,
        "colSize": "col-md-4",
        "loadWhen": "cityAdded",
        "acl": {
          "allow": [],
          "reject": []
        },
        "priority": 2,
        "id": "AddCity",
        "label": "Add City"
      }
    },
    "area": {
      "type": "belongsTo",
      "model": "Area",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Area",
        "searchProp": "name",
        "type": "smartSelect",
        "readOnly": false,
        "inline": true,
        "colSize": "col-md-4",
        "loadWhen": "areaLoaded",
        "acl": {
          "allow": [],
          "reject": []
        }
      }
    },
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Brand",
        "searchProp": "name",
        "type": "smartSelect",
        "readOnly": true,
        "inline": true,
        "colSize": "col-md-4",
        "loadWhen": "dummyBrandAdded",
        "acl": {
          "allow": [],
          "reject": []
        },
        "id": "AddBrand",
        "label": "Add Brand"
      }
    }
  },
  "acls": [],
  "methods": {}
}
