{
  "name": "VehicleDetail",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "registrationNumber": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Registration Number",
          "id": "registration Number"
        }
      }
    },
    "registeredName": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Registered Name",
          "id": "registeredName"
        }
      }
    },
    "showroomName": {
      "type": "string",
      "required": false,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Showroom Name",
          "id": "showroomName"
        }
      }
    },
    "workshopName": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Workshop Name",
          "id": "workshopName"
        }
      }
    },
    "status": {
      "type": "string",
      "default": "active"
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    },
    "vehicleType": {
      "type": "string"
    },
    "vehicleInsuranceProof": {
      "type": "object",
      "required": false,
      "template": {
        "type": "singleFileUpload",
        "templateOptions": {
          "label": "Add Vehicle Insurance Proof",
          "containerName": "autoboxsnaphy",
          "containerModel": "Container",
          "bind": true,
          "fileDataSource": "Image",
          "onImageUpdate": {
            "deletePrevious": true
          }
        }
      }
    },
    "vehicleRegistrationProof": {
      "type": "object",
      "required": false,
      "template": {
        "type": "singleFileUpload",
        "templateOptions": {
          "label": "Add Vehicle Registration Proof",
          "containerName": "autoboxsnaphy",
          "containerModel": "Container",
          "bind": true,
          "fileDataSource": "Image",
          "onImageUpdate": {
            "deletePrevious": true
          }
        }
      }
    },
    "pucProof": {
      "type": "object",
      "required": false,
      "template": {
        "type": "singleFileUpload",
        "templateOptions": {
          "label": "Add puc Proof",
          "containerName": "autoboxsnaphy",
          "containerModel": "Container",
          "bind": true,
          "fileDataSource": "Image",
          "onImageUpdate": {
            "deletePrevious": true
          }
        }
      }
    },
    "yearOfPurchase":{
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "showroom": {
      "type": "belongsTo",
      "model": "Showroom",
      "foreignKey": ""
    },
    "workshop": {
      "type": "belongsTo",
      "model": "Workshop",
      "foreignKey": ""
    },
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Customer",
        "searchProp": "email",
        "inline": true,
        "colSize": "col-md-4",
        "box": "customer",
        "id": "AddCustomer",
        "label": "Customer"
      }
    },
    "vehicleInfo": {
      "type": "belongsTo",
      "model": "VehicleInfo",
      "foreignKey": ""
    },
    "insurance": {
      "type": "belongsTo",
      "model": "Insurance",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
