{
  "name": "Offer",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "expiredOn": {
      "type": "date",
      "required": true
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "city": {
      "type": "belongsTo",
      "model": "City",
      "foreignKey": ""
    },
    "area": {
      "type": "belongsTo",
      "model": "Area",
      "foreignKey": ""
    },
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": ""
    },
    "dealer": {
      "type": "belongsTo",
      "model": "Dealer",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
