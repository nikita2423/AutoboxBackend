{
  "name": "Color",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter name",
          "id": "colorName"
        }
      }
    },
    "colorCode": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter Color Code",
          "id": "colorCode"
        }
      }
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "active",
      "template": {
        "type": "selectString",
        "templateOptions": {
          "label": "Status of Color",
          "id": "trimStatus",
          "priority": 6,
          "options": [
            "active",
            "inactive"
          ]
        }
      }
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "cars": {
      "type": "hasAndBelongsToMany",
      "model": "Car",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
