{
  "name": "Trim",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter name",
          "id": "trimName"
        }
      }
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "active",
      "template": {
        "type": "selectString",
        "templateOptions": {
          "label": "Status of Trim",
          "id": "trimStatus",
          "priority": 6,
          "options": [
            "active",
            "inactive"
          ]
        }
      }
    },
    "trimNumber": {
      "type": "string",
      "required": true
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
