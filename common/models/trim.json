{
  "name": "Trim",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": false,
      "default": "NA",
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter name",
          "id": "trimName"
        }
      }
    },
    "status": {
      "type": "string",
      "required": false,
      "default": "active",
      "template": {
        "type": "selectString",
        "templateOptions": {
          "label": "Status of Trim",
          "id": "trimStatus",
          "priority": 6,
          "options": [
            "active",
            "inactive"
          ]
        }
      }
    },
    "trimNumber": {
      "type": "string",
      "required": false
    },
    "added": {
      "type": "date",
      "required": false,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Brand",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "create": false,
        "type": "smartSelect",
        "id": "AddBrand",
        "hide": false,
        "label": "Brand"
      }
    },
    "carModel": {
      "type": "belongsTo",
      "model": "CarModel",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add CarModel",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "create": false,
        "type": "smartSelect",
        "id": "AddCarModel",
        "hide": false,
        "label": "Car Model"
      }
    },
    "fuel": {
      "type": "belongsTo",
      "model": "Fuel",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Fuel",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "create": false,
        "type": "smartSelect",
        "id": "AddFuel",
        "hide": false,
        "label": "Fuel"
      }

    },
    "gearBox": {
      "type": "belongsTo",
      "model": "GearBox",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Gearbox",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "create": false,
        "type": "smartSelect",
        "id": "AddGearbox",
        "hide": false,
        "label": "Gearbox"
      }
    }
  },
  "acls": [],
  "methods": {}
}
