{
  "name": "Brand",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter name",
          "id": "brandName"
        }
      }
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "active",
      "template": {
        "type": "selectString",
        "templateOptions": {
          "label": "Status of Brand",
          "id": "brandStatus",
          "priority": 6,
          "options": [
            "active",
            "inactive"
          ]
        }
      }
    },
    "trending": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "brandNumber": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "object",
      "required": true,
      "template": {
        "type": "singleFileUpload",
        "templateOptions": {
          "label": "Add Brand Image",
          "containerName": "AMAZON_CONTAINER_NAME",
          "containerModel": "Container",
          "bind": true,
          "fileDataSource": "Image",
          "onImageUpdate": {
            "deletePrevious": true
          }
        }
      }
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
