{
  "name": "Car",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "carNumber": {
      "type": "string",
      "required": true
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": ""
    },
    "carModel": {
      "type": "belongsTo",
      "model": "CarModel",
      "foreignKey": ""
    },
    "fuel": {
      "type": "belongsTo",
      "model": "Fuel",
      "foreignKey": ""
    },
    "trim": {
      "type": "belongsTo",
      "model": "Trim",
      "foreignKey": ""
    },
    "gearBox": {
      "type": "belongsTo",
      "model": "GearBox",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
