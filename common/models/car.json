{
  "name": "Car",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "active",
      "template": {
        "type": "selectString",
        "templateOptions": {
          "label": "Status of Car",
          "id": "brandStatus",
          "priority": 6,
          "options": [
            "active",
            "inactive"
          ]
        }
      }
    },
    "carNumber": {
      "type": "string",
      "required": false
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Brand",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "create": false,
        "type": "smartSelect",
        "id": "AddBrand",
        "hide": false,
        "label": "Brand"
      }
    },
    "carModel": {
      "type": "belongsTo",
      "model": "CarModel",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Car Model",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "create": false,
        "type": "smartSelect",
        "id": "AddCarModel",
        "hide": false,
        "label": "Car Model"
      }
    },
    "fuel": {
      "type": "belongsTo",
      "model": "Fuel",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Fuel",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "type": "smartSelect",
        "create": false,
        "id": "AddFuel",
        "hide": false,
        "label": "Fuel"
      }
    },
    "trim": {
      "type": "belongsTo",
      "model": "Trim",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Trim",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "type": "smartSelect",
        "create": false,
        "id": "AddTrim",
        "hide": false,
        "label": "Trim"
      }
    },
    "gearBox": {
      "type": "belongsTo",
      "model": "GearBox",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add GearBox",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "type": "smartSelect",
        "create": false,
        "id": "AddGearBox",
        "hide": false,
        "label": "GearBox"
      }
    },
    "colors": {
      "type": "hasAndBelongsToMany",
      "model": "Color",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Colors",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "create": false,
        "load": true,
        "id": "AddColors",
        "hide": false,
        "label": "Add Colors"
      }
    }
  },
  "acls": [],
  "methods": {}
}
