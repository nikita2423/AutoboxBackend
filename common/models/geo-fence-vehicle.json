{
  "name": "GeoFenceVehicle",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "deviceIMEI": {
      "type": "string"
    },
    "kilometers": {
      "type": "string"
    },
    "modelName": {
      "type": "string"
    },
    "registerNumber": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "active",
      "template": {
        "type": "selectString",
        "templateOptions": {
          "label": "Status of Car",
          "id": "brandStatus",
          "priority": 6,
          "options": [
            "active",
            "inactive"
          ]
        }
      }
    },
    "homeLocation": {
      "type": "geopoint",
      "required": true,
      "template": {
        "type": "objectValue",
        "templateOptions": {
          "fields": [
            {
              "type": "input",
              "key": "lat",
              "templateOptions": {
                "type": "text",
                "label": "Enter latitude",
                "id": "latitude"
              }
            },
            {
              "type": "input",
              "key": "lng",
              "templateOptions": {
                "type": "text",
                "label": "Enter longitude",
                "id": "longitude"
              }
            }
          ]
        }
      }
    },
    "homeLocationAddress": {
      "type": "string"
    },
    "added": {
      "type": "date",
      "required": false,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": ""
    },
    "vehicleDetail": {
      "type": "belongsTo",
      "model": "VehicleDetail",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
