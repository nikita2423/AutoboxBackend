{
  "name": "Emergency",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "contactNumber": {
      "type": "string",
      "required": true
    },
    "latlong": {
      "type": "geopoint",
      "required": true
    },
    "emergencyNumber": {
      "type": "string",
      "required": true
    },
    "added": {
      "type": "date",
      "required": true,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "city": {
      "type": "belongsTo",
      "model": "City",
      "foreignKey": ""
    },
    "area": {
      "type": "belongsTo",
      "model": "Area",
      "foreignKey": ""
    },
    "emergencyCategory": {
      "type": "belongsTo",
      "model": "EmergencyCategory",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
