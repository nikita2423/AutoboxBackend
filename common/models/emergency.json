{
  "name": "Emergency",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter name",
          "id": "emergencyName"
        }
      }
    },
    "contactNumber": {
      "type": "string",
      "required": false,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter Contact Number",
          "id": "emergencyContact"
        }
      }
    },
    "latlong": {
      "type": "geopoint",
      "required": true,
      "template": {
        "type": "objectValue",
        "templateOptions": {
          "fields": [
            {
              "type": "input",
              "key": "lat",
              "templateOptions": {
                "type": "text",
                "label": "Enter latitude",
                "id": "latitude"
              }
            },
            {
              "type": "input",
              "key": "lng",
              "templateOptions": {
                "type": "text",
                "label": "Enter longitude",
                "id": "longitude"
              }
            }
          ]
        }
      }
    },
    "emergencyNumber": {
      "type": "string",
      "required": false
    },
    "added": {
      "type": "date",
      "required": false,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "city": {
      "type": "belongsTo",
      "model": "City",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add City",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "create": false,
        "id": "AddCity",
        "hide": false,
        "label": "Add City"
      }
    },
    "area": {
      "type": "belongsTo",
      "model": "Area",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Area",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "create": false,
        "id": "AddArea",
        "hide": false,
        "label": "Add Area"
      }
    },
    "emergencyCategory": {
      "type": "belongsTo",
      "model": "EmergencyCategory",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Category",
        "searchProp": "name",
        "priority": 2,
        "search": true,
        "create": false,
        "id": "AddCategory",
        "hide": false,
        "label": "Add Category"
      }
    }
  },
  "acls": [],
  "methods": {}
}
