{
  "name": "School",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter name",
          "id": "schoolName"
        }
      }
    },
    "email": {
      "type": "string",
      "required": true,
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "email",
          "label": "Enter School Email",
          "id": "schoolEmail",
          "priority": 7
        }
      }
    },
    "username": {
      "type": "string",
      "template": {
        "type": "input",
        "templateOptions": {
          "type": "text",
          "priority": 3,
          "label": "Enter Username",
          "id": "username"
        }
      }
    },
    "added": {
      "type": "date",
      "required": false,
      "defaultFn": "now",
      "template": {
        "type": "dummy"
      }
    },
    "updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "area": {
      "type": "belongsTo",
      "model": "Area",
      "foreignKey": ""
    },
    "city": {
      "type": "belongsTo",
      "model": "City",
      "foreignKey": ""
    }
  },
  "acls": [
